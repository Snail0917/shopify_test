{% comment %}Fetch selected product from the product list{% endcomment %} {%- assign product = all_products[section.settings.plus_product] -%}  {% comment %}If product not selected then consider as on boarding{% endcomment %} {% if product == empty %}   {%- assign section_onboarding = true -%} {% endif %}  {% comment %}Get product first variant, or deep linked one{% endcomment %} {%- assign current_variant = product.selected_or_first_available_variant -%} {%- assign product_image_size = '530x' -%} {%- assign product_image_scale = '2' -%} {%- assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media -%}  <script> /* ----------------------- START: Script for plus-product ----------------------- */ </script> <link rel="stylesheet" href="https://firebasestorage.googleapis.com/v0/b/plus-page.appspot.com/o/assets%2Fmagnific-popup.css?alt=media&token=c584962b-4721-4728-8a29-64986f51b579"> <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script> <script src="https://firebasestorage.googleapis.com/v0/b/plus-page.appspot.com/o/assets%2Fjquery.magnific-popup.min.js?alt=media&token=744223c9-7334-41d7-ae9d-8a3844d508b5"></script> <script> $(document).ready(function($) {     /* Fetch sectionId from liquid */   const sectionId = "{{ section.id }}";   console.log('sectionId:', sectionId);    /* Fetch selected product object from liquid */   let product;   {% if product != empty %}     product = {{ product | json }};   {% endif %}   console.log('product:', product);    /* START: Show large image when clicked thumb */   $('.product-thumb-' + sectionId).on('click', function(e) {     e.preventDefault();     $('#video-' + sectionId).hide();     const previewImageUrl = $(this).attr('data-preview-image-url');     $('#large-image-' + sectionId).attr('src', previewImageUrl);     $('#large-image-' + sectionId).show();   });   /* END: Show large image when clicked thumb */    /* START: Show video component when clicked video thumb */   /* i.e. hide product large image and show video component */   $('.video-thumb-' + sectionId).on('click', function(e) {     e.preventDefault();     $('#large-image-' + sectionId).hide();     $('#video-' + sectionId).show();   });   /* END: Show video component when clicked video thumb */    /* START: Change price whenever any option changed from dropdown */   $('.plus-product-variant-' + sectionId).on('change', function(e){     /* Fetch selected value for each option */      /* Product have maximum three type of options e.g. Colour, Size, Weight etc. */     let option1 = null; let option2 = null; let option3 = null;      product.options.forEach( function(option, index) {       if (index === 0 ) { option1 = $('#SingleOptionSelector-' + sectionId + '-0 option:selected').val() };       if (index === 1 ) { option2 = $('#SingleOptionSelector-' + sectionId + '-1 option:selected').val() };       if (index === 2 ) { option3 = $('#SingleOptionSelector-' + sectionId + '-2 option:selected').val() };     });     console.log('option1:', option1);     console.log('option2:', option2);     console.log('option3:', option3);       /* Fetch matching variant for selected options */     let selectedVariant = null;     product.variants.forEach(function(variant, index){        if (variant.option1 === option1 && variant.option2 === option2 && variant.option3 === option3) {         selectedVariant = variant;       }     });     console.log('selectedVariant:', selectedVariant);      /* Update selected variant price and discount info within ui */     if (selectedVariant !== null) {        /* Show product price within ui */       const price = selectedVariant.price/100;       $('#price-' + sectionId).html( '$' + price.toFixed(2));        /* If product at discounted show Sale info, otherwise hide it */       if( selectedVariant.compare_at_price > selectedVariant.price ) {         const compareAtPrice = selectedVariant.compare_at_price/100;         $('#compare-at-price-' + sectionId).html('$' + compareAtPrice.toFixed(2));         $('#compare-at-price-' + sectionId).show();         $('#sale-' + sectionId).show();       }else {         $('#compare-at-price-' + sectionId).hide();         $('#sale-' + sectionId).hide();       }     }          /* Set selected variant id within add to cart or buy now form element */     if (selectedVariant !== null) {       $('#ProductSelect-' + sectionId + ' option:selected').attr("selected",null);       $('#ProductSelect-' + sectionId + ' option[value="' + selectedVariant.id + '"]').attr("selected","selected");     }      /* Disable add to cart button if variant not available for sale (i.e. sold out) */     /* Also change button text Sold Out, change text color grey etc. */     if (selectedVariant !== null) {       if (selectedVariant.available === false) {         $('#add-cart-button-' + sectionId).html('Sold Out');         $('#add-cart-button-' + sectionId).attr('disabled', 'disabled');         $('#add-cart-button-' + sectionId).css({'color':'#cdcdcd','cursor':'default'});       }else {         $('#add-cart-button-' + sectionId).html('Add to Cart');         $('#add-cart-button-' + sectionId).attr('disabled', null);         $('#add-cart-button-' + sectionId).css({'color':'#000000','cursor':'pointer'});       }     }      /* If featured image exist for the variant, then show it as large image */     /* At this moment video component may be visible, so we will hide it */     if (selectedVariant !== null) {        if (selectedVariant.featured_image) {          $('#video-' + sectionId).hide();          $('#large-image-' + sectionId).attr('src', selectedVariant.featured_image.src);          $('#large-image-' + sectionId).show();        }     }    });   /* END: Change price whenever any option changed from dropdown */       /* START: Show lightbox gallery when clicked product large image */   /* Help: https://dimsemenov.com/plugins/magnific-popup/documentation.html */   const productImages = [];   if (product) {     product.media.forEach( function(item, index) {       productImages.push({ src: item.preview_image.src });     });   }   /* Collect video info from settings and combine with product images */   const videoType = "{{ section.settings.video_type }}";   const videoUrl = "{{ section.settings.video_url }}";   const mp4Url = "{{ section.settings.mp4_url }}";   if (videoType === 'mp4' && mp4Url !== '') {     productImages.push({src: mp4Url, type: 'iframe'});   } else if (videoType === 'yt_vimeo' && videoUrl !== '') {     productImages.push({src: videoUrl, type: 'iframe'});   }   console.log('productImages:', productImages);   $('#large-image-' + sectionId).magnificPopup({     items: productImages,     gallery: {       enabled: true,       navigateByImgClick: true,     },     type: 'image'   });     /* END: Show lightbox gallery when clicked product large image */       /* START: Ajax based add to cart functionality */   /* HELP: https://shopify.dev/tutorials/develop-theme-getting-started-using-ajax-api */   const addToCartBehaviour = "{{ section.settings.add_to_cart_behaviour }}";   $('#add-cart-button-' + sectionId).on('click', function(e) {     /* If add to cart behaviour is ajax then prevent default form submit action and      perform ajax based add to cart functionality using jquery post method, otherwise      return so it will continue to perform traditional form submit. */     if (addToCartBehaviour === 'ajax') {        e.preventDefault();     } else {       return;     }      /* Disable add to cart button, so user can not tap it twice */     $('#add-cart-button-' + sectionId).attr('disabled', 'disabled');     $('#add-cart-button-' + sectionId).css({'color':'#cdcdcd','cursor':'default'});       /* Read quantity and selected variant Id from the form element */     const qty = $('#Quantity-' + sectionId).val();     const variantId = $('#ProductSelect-' + sectionId + ' option:selected').val();     const postData = {       items : [{          quantity: qty,          id: variantId        }]     };     console.log('postData:', postData);       /* Add item to cart via post request */     $.post("/cart/add.js", postData, function(data, status) {       const responseData = JSON.parse(data);       console.log("(After add to cart) data: ", responseData, " status: ", status);              if (status === 'success' && responseData.items ) {         /* Read added item info received from api call */         const item = responseData.items[0];                  /* Show item added to cart success message */         $('#added-qty-' + sectionId).html(qty);         $('#added-title-' + sectionId).html(item.title);         $('#added-success-message-' + sectionId).show(1000);                  /*  Hide success message after x seconds */         setTimeout( function(){            $('#added-success-message-' + sectionId).hide(1000);          }, 6000);        } else {         /*  Show error message */         $('#added-error-message-' + sectionId).show(1000);           /*  Hide error message after x seconds */         setTimeout( function(){            $('#added-error-message-' + sectionId).hide(1000);          }, 6000);        }        /* Enable add to cart button */       $('#add-cart-button-' + sectionId).attr('disabled', null);       $('#add-cart-button-' + sectionId).css({'color':'#000000','cursor':'pointer'});     });    });   /* END: Ajax based add to cart functionality */  }); </script> <script> /* ----------------------- END: Script for plus-product ------------------------- */ </script>  <div style="width: 100%; height: {{section.settings.top_space}}px;"></div> <div class="plus-product"       style="margin: auto;             max-width: {{ section.settings.content_max_width }}px;             padding-left: {{ section.settings.content_left_right_padding }}px;             padding-right: {{ section.settings.content_left_right_padding }}px;             border: 0px solid #ff0000;             ">    {% capture mp4_video_default %}   <video controls=true style="width: 100%;object-fit: cover;" >     <source src="https://cdn.shopify.com/s/files/1/0248/2113/7485/files/Street_-_19627.mp4" type="video/mp4" >   </video>   {% endcapture %}    {% capture mp4_video %}   <video controls=true style="width: 100%;object-fit: cover;" >     <source src="{{ section.settings.mp4_url }}" type="video/mp4" >   </video>   {% endcapture %}    {% capture youtube_video_default %}   <div class="video-wrapper"         style="position: relative;               overflow: hidden;               max-width: 100%;               padding-bottom: 56.25%;               height: 0;               height: auto;               ">     <iframe src="//www.youtube.com/embed/EngW7tLk6R8?rel=0&showinfo=0&vq=720"             width="850" height="480" frameborder="0" allowfullscreen             style="position: absolute; top: 0; left: 0; width: 100%; height: 100%;">     </iframe>   </div>   {% endcapture %}    {% capture youtube_video %}   <div class="video-wrapper"         style="position: relative;               overflow: hidden;               max-width: 100%;               padding-bottom: 56.25%;               height: 0;               height: auto;               ">     <iframe src="//www.youtube.com/embed/{{ section.settings.video_url.id }}?rel=0&showinfo=0&vq=720"             width="850" height="480" frameborder="0" allowfullscreen             style="position: absolute; top: 0; left: 0; width: 100%; height: 100%;">     </iframe>   </div>   {% endcapture %}    {% capture vimeo_video_default %}   <div class="video-wrapper"         style="position: relative;               overflow: hidden;               max-width: 100%;               padding-bottom: 56.25%;               height: 0; height: auto;               ">     <iframe src="//player.vimeo.com/video/253989945?color={{ settings.color_secondary | remove: "#" }}&byline=0&portrait=0&badge=0"             width="850" height="480" frameborder="0" allowfullscreen             style="position: absolute; top: 0; left: 0; width: 100%; height: 100%;">     </iframe>   </div>   {% endcapture %}    {% capture vimeo_video %}   <div class="video-wrapper"         style="position: relative;               overflow: hidden;               max-width: 100%;               padding-bottom: 56.25%;               height: 0; height: auto;               ">     <iframe src="//player.vimeo.com/video/{{ section.settings.video_url.id }}?color={{ settings.color_secondary | remove: "#" }}&byline=0&portrait=0&badge=0"             width="850" height="480" frameborder="0" allowfullscreen             style="position: absolute; top: 0; left: 0; width: 100%; height: 100%;">     </iframe>   </div>   {% endcapture %}    {% capture video_component %}   <div id="video-{{ section.id }}"        style="padding: 0px;                border: 0px solid #ff0000;                display: none;               ">       {% if section.settings.video_type == 'mp4' and section.settings.mp4_url == blank %}         {{ mp4_video_default }}       {% endif %}        {% if section.settings.video_type == 'mp4' and section.settings.mp4_url != blank %}         {{ mp4_video }}       {% endif %}        {% if section.settings.video_type == 'yt_vimeo' and section.settings.video_url == blank %}         {{ youtube_video_default }}       {% endif %}        {% if section.settings.video_type == 'yt_vimeo'              and section.settings.video_url != blank              and section.settings.video_url.type == "youtube" %}         {{ youtube_video }}       {% endif %}        {% if section.settings.video_type == 'yt_vimeo'              and section.settings.video_url != blank              and section.settings.video_url.type == "vimeo" %}         {{ vimeo_video }}       {% endif %}   </div>   {% endcapture %}    {% capture thumb_images %}   <div style="margin: 10px 0px 0px 0px; border: 0px solid #ff0000;" >     {%- for media in product.media -%}       <a           href="#"          class="product-thumb-{{ section.id }}"          data-section-id="{{ section.id }}"          data-media-id="{{ media.id }}"         data-media-type="{{ media.media_type }}"         data-preview-image-url="{{ media.preview_image | img_url: '530x' }}"        >          <img            src="{{ media.preview_image | img_url: '110x110', scale: '1' }}"            alt="{{ media.alt }}"            style="width: 80px; border: 1px solid #cdcdcd; margin: 4px 4px 4px 4px;"          />       </a>     {%- endfor -%}     {%- if section.settings.video_thumb_image != nil -%}       <a href="#" class="video-thumb-{{ section.id }}" >          <img            src="{{ section.settings.video_thumb_image | img_url: '110x110', scale: '1' }}"            alt="Video - {{ product.title }}"            style="width: 80px; border: 1px solid #cdcdcd; margin: 4px 4px 4px 4px;"          />       </a>     {%- else -%}       {%- if section.settings.video_url != blank or section.settings.mp4_url != blank -%}         <a href="#" class="video-thumb-{{ section.id }}" >           <div style="display: table-cell;                        padding: 5px 0px 5px 0px;                        width: 80px;                        border: 1px solid #cdcdcd;                        text-align: center;                        vertical-align: middle;                        font-size: 12px;                      ">             Product<br />Video           </div>         </a>       {% endif %}       {% endif %}   </div>   {% endcapture %}    {% capture empty_image_box %}     <div style="width: 100%;                 height: 400px;                  border: 1px solid #cdcdcd;                 background: #efefef;                  text-align: center;                  vertical-align: middle;                  font-size: 16px;                ">     </div>   {% endcapture %}    {% capture image_layout %}     <div class="plus-product__image_CLOSED"          style="flex: 50%;                 max-width: 100%;                 height: 0;                 height: auto;                 border: 0px solid #00ff00;                 ">              {{ video_component }}        {% comment %}If product not selected then render empty box for large image{% endcomment %}       {% if product == empty %}       {{ empty_image_box }}       {% endif %}        {% unless section_onboarding %}         <img            src="{{ featured_media | img_url: product_image_size, scale: product_image_scale }}"            alt="{{ featured_media.alt }}"            id="large-image-{{ section.id }}"            style="width: 100%;cursor: pointer;"            />       {% endunless %}              {{ thumb_images }}      </div>   {% endcapture %}    {% capture price_html %}     {% unless section_onboarding %}       {%- assign price = current_variant.price -%}       {%- assign compare_at_price = current_variant.compare_at_price -%}       {%- assign available = current_variant.available -%}     {% else %}       {%- assign price = 0 -%}       {%- assign compare_at_price = 0 -%}       {%- assign available = true -%}     {% endunless %}     <h4>       Price: <span id="price-{{ section.id }}" >{{ price | money }}</span>       <span id="compare-at-price-{{ section.id }}" style="text-decoration: line-through;" >         {% if compare_at_price > price %}{{ compare_at_price | money }}{% endif %}       </span>       <span id="sale-{{ section.id }}">         {% if compare_at_price > price %}(Sale){% endif %}       </span>     </h4>   {% endcapture %}     {% comment %}Item added to cart succcess message {% endcomment %}   {% capture add_cart_success_message %}     <div        id="added-success-message-{{ section.id }}"        style="width: {{ section.settings.button_width }}%;             margin-top: 10px;              margin-bottom: 10px;              padding: 10px;              border: 1px solid #cdcdcd;              display: none;              "      >       Added <b>(<span id="added-qty-{{ section.id }}" >??</span>)</b>        <b><span id="added-title-{{ section.id }}" >??</span></b>        to your cart.     </div>   {% endcapture %}     {% comment %}Item added to cart error message {% endcomment %}   {% capture add_cart_error_message %}     <div        id="added-error-message-{{ section.id }}"        style="width: {{ section.settings.button_width }}%;             margin-top: 10px;              margin-bottom: 10px;              padding: 10px;              border: 1px solid #ff0000;              display: none;              "     >       Error while add to cart, please try again.     </div>   {% endcapture %}     {% comment %}   We can provide various class name that will be used by cart form.   At present there is no special use case but we kept it for future use.   {% endcomment %}   {% capture form_classes %}     product-form-{{ section.id }}   {% endcapture %}     {% comment %}   This consist options/quantity dropdown, add to cart, and buy now button etc.   Add to cart and buy now button require form input element so we put all inpu    ui element within shopify form.   {% endcomment %}   {% capture option_html %}     {% form 'product', product, class:form_classes, novalidate: 'novalidate', data-product-form: '' %}       {% comment %}       Render options(color, size etc.) dropdown for current product. Whenever        user will select options from this dropdown we will change product        price and discount info within ui.       {% endcomment %}       {% unless product.has_only_default_variant %}         {% for option in product.options_with_values %}           <div style="margin-bottom: 10px; border: 0px solid #ff0000;" >              <label for="SingleOptionSelector-{{ section.id }}-{{ forloop.index0 }}" >               {{ option.name }}             </label>             <select class="plus-product-variant-{{ section.id }}"                      id="SingleOptionSelector-{{ section.id }}-{{ forloop.index0 }}"                      data-index="option{{ forloop.index }}"                      style="width: {{ section.settings.variant_width }}%;;                           padding: 10px;                           text-decoration: none;                           text-align: center;                           text-transform: uppercase;                           vertical-align: middle;                           cursor: pointer;                           border: 1px solid #cdcdcd;                           color: #000000;                           font-size: 20;                           font-weight: '600';                           "             >               {% for value in option.values %}                 <option value="{{ value | escape }}"                    {% if option.selected_value == value %}selected="selected"{% endif %}                  >                    {{ value }}                 </option>               {% endfor %}             </select>           </div>         {% endfor %}       {% endunless %}              {% comment %}       Create hidden dropdown for product variant list, whenever user will select        option(Size, color) from dropdown, we will find variant for the selected options        and set that variant id as selected. This hidden dropdown is used for add to cart       and buy now button purpose.       {% endcomment %}       <select name="id" id="ProductSelect-{{ section.id }}" style="display: none;">         {% for variant in product.variants %}           <option value="{{ variant.id }}"             {%- if variant == current_variant %}              selected="selected"              {%- endif -%}           >             {{ variant.title }} {%- if variant.available == false %} - Sold Out{% endif %}           </option>         {% endfor %}       </select>        {% comment %}       Show quantity selector if enabled within section settings.       {% endcomment %}       <div          style="margin-bottom: 10px;                border: 0px solid #ff0000;               {% if section.settings.show_quantity_selector == false %}display: none;{% endif %}               "       >         <label for="Quantity-{{ section.id }}">           {% assign quantityTranslated = 'products.product.quantity' | t %}           {% if quantityTranslated contains "translation missing" %} Quantity {% else %} {{ quantityTranslated }} {% endif %}         </label>         <input type="number" id="Quantity-{{ section.id }}"           name="quantity" value="1" min="1" pattern="[0-9]*"           style="width: {{ section.settings.quantity_width }}%;                 padding: 10px;                 text-decoration: none;                 text-align: center;                 text-transform: uppercase;                 vertical-align: middle;                 cursor: pointer;                 border: 1px solid #cdcdcd;                 color: #000000;                 font-size: 20;                 font-weight: '600';                 "            data-quantity-input          >       </div>        {% comment %} Add to cart button{% endcomment %}       <div style="margin-top: 10px; margin-bottom: 10px; border: 0px solid #ff0000;" >         <button           id="add-cart-button-{{ section.id }}"            type="submit"            name="add"            style="display: inline-block;                  width: {{ section.settings.button_width }}%;                  padding: 10px;                  text-decoration: none;                  text-align: center;                  text-transform: uppercase;                  vertical-align: middle;                  cursor: pointer;                  border: 1px solid #cdcdcd;                  color: #000000;                  font-size: 20;                  font-weight: '600';                 "          >           {% unless current_variant.available %}             {% assign soldOutTranslated = 'products.product.sold_out' | t %}             {% if soldOutTranslated contains "translation missing" %} Sold Out {% else %} {{ soldOutTranslated }} {% endif %}           {% else %}             {% assign addToCartTranslated = 'products.product.add_to_cart' | t %}             {% if addToCartTranslated contains "translation missing" %} Add to Cart {% else %} {{ addToCartTranslated }} {% endif %}           {% endunless %}         </button>       </div>        {% comment %} Item added to cart success message {% endcomment %}       {{ add_cart_success_message }}        {% comment %} Item added to cart error message {% endcomment %}       {{ add_cart_error_message }}        {% comment %}       Show dynamically payment (Buy Now) button if enabled within section settings.       {% endcomment %}       {% if section.settings.enable_payment_button %}         <div style="border: 0px solid #00ff00; width: {{ section.settings.button_width }}%;" >           {{ form | payment_button }}         </div>       {% endif %}      {% endform %}   {% endcapture %}       {% capture text_layout %}     <div class="plus-product__text"          style="flex: 50%;                 padding-{{ section.settings.image_align }}: 20px;                 border: 0px solid #0000ff;                 ">              <h1 style="text-transform: none;" >         {% unless product == empty %}{{ product.title }}{% else %}{{ 'Product title' }}{% endunless %}       </h1>              {% if section.settings.show_vendor and product %}       <h2>{{ product.vendor }}</h2>       {% endif %}              {{ price_html }}        {% if product != empty %}       {{ option_html }}       {% endif %}        <div style="margin-top: 10px;" >{{ product.description }}</div>     </div>   {% endcapture %}    <div class="plus-product__row" style="display: flex; flex-wrap: wrap;">     {% if section.settings.image_align == 'left' %}       {{ image_layout }}     {% endif %}     {{ text_layout }}     {% if section.settings.image_align == 'right' %}       {{ image_layout }}     {% endif %}   </div>  </div> <div style="width: 100%; height: {{section.settings.bottom_space}}px;"></div>   {% schema %}   {     "name": {       "en": "Plus - Product",       "fr": "Plus - Product