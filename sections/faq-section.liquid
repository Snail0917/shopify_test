<script>
  $(".faq-tab-head p").click(function() {
    var index = $(this).index();
    $(this).parents(".faq-content").find(".tab_collection").hide();
    $(this).parents(".faq-content").find(".tab_collection").eq(index).show();
    $(this).parents(".faq-content").find('.faq-tab-head p').removeClass('current');
    $(this).addClass('current');
    console.log(index)
  });
  
  $('.list-tabs h6').click(function(){
  	$(this).siblings('p').slideToggle();
    $(this).toggleClass('border-none');
    $(this).children().toggleClass('transform_180');
    $(this).parent().toggleClass('backgroung-white');
    $(this).parent().siblings().removeClass('backgroung-white');
    $(this).parent().siblings().children('h6').children().removeClass('transform_180');
    $(this).parent().siblings().children('h6').removeClass('border-none');
    $(this).parents('.list-tabs').siblings().children('p').slideUp();
  });
</script>


